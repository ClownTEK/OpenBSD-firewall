; internal LAN

$ORIGIN .

$TTL 60

lan	IN		SOA	obr.lan. root.obr.lan. (
				2015033000	; serial
				3h		; refresh
				15m		; retry
				1w		; expire
				60		; minimum TTL
			)

			IN	NS		obr.lan.

$ORIGIN lan.


echo ifnet: bringing up interfaces

ifconfig re0 192.168.100.2 netmask 255.255.255.0 description "S18 protected DMZ"
ifconfig re1 192.168.0.1 netmask 255.255.255.0 description "S18 internal"
ifconfig re2 192.168.10.1 netmask 255.255.255.0 description "S18 wlan"
ifconfig vlan1 create
ifconfig vlan1 192.168.30.1 netmask 255.255.255.0 vlan 3 vlandev re2 description "S18 guest wlan"
ifconfig vlan2 create
ifconfig vlan2 192.168.20.1 netmask 255.255.255.0 vlan 2 vlandev re2 description "S18 restricted wlan"

echo route: adding default route...

route add default 192.168.100.1
